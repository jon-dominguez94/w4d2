<h1>Here are our cats!</h1>

<table>
  <tr>
    <td>Name</td>
    <td>Birth Date</td>
    <td>Color</td>
    <td>Sex</td>
    <td>Description</td>
  </tr>
<% @cats.each do |cat| %>
  <tr>
    <td><%= cat.name %></td>
    <td><%= cat.birth_date %></td>
    <td><%= cat.color %></td>
    <td><%= cat.sex %></td>
    <td><%= cat.description %></td>
  </tr>
<% end %>
</table>

